<!-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
<!-- (If only used in success in condition then create in array then automatically flash green color are generate used in condition if length) -->

<% if(success && success.length>0){%>
    <div class="alert alert-success alert-dismissible col-6 offset-3" role="alert">
        <%= success %>
        <button type="button" class="btn-close" data-bs-dismiss="alert">
          
          </button>
      </div>
      <%}%>
<!-- 
 ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
 (If listing are not listing)
 -->

 <% if(error && error.length>0){%>
    <div class="alert alert-danger alert-dismissible col-6 offset-3" role="alert">
        <%=error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert">
          
          </button>
      </div>
      <%}%>